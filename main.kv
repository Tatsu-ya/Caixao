<TelaCriarConta>:
    Screen:
        MDFloatLayout:
            orientation: "vertical"
            MDTopAppBar:
                elevation: 3
                title: '[size=20]                             Criar Conta[/size]'
                anchor_title: 'center'
                pos_hint: { "center_y": .97}
                md_bg_color: '#2d2734'
        MDIconButton:
            icon: "arrow-left-bold-circle-outline"
            pos_hint: {"center_x": .05, "center_y": .97}
            theme_icon_color: "Custom"
            icon_color: "white"
            on_release: root.manager.current = "TelaLogin"

        MDFloatLayout:
            MDTextField:
                size_hint_x: .8
                hint_text: 'Usuário:'
                pos_hint: {'center_x': .5, 'center_y': .75}
                icon_left: "account-outline"
                line_color_focus: "blue"
                hint_text_color_normal: "blue"

            MDTextField:
                id: usuario
                size_hint_x: .8
                hint_text: 'E-mail:'
                validator: 'email'
                pos_hint: {'center_x': .5, 'center_y': .65}
                icon_left: "email"
                line_color_focus: "blue"
                hint_text_color_normal: "blue"

            MDTextField:
                id: senha
                size_hint_x: .8
                hint_text: 'Senha:'
                max_text_length: 8
                pos_hint: {'center_x': .5, 'center_y': .55}
                line_color_focus: "blue"
                password: True
                icon_left: "key-variant"
                hint_text_color_normal: "blue"

            MDTextField:
                id: confsenha
                size_hint_x: .8
                hint_text: 'Confirme a Senha:'
                max_text_length: 8
                pos_hint: {'center_x': .5, 'center_y': .45}
                line_color_focus: "blue"
                password: True
                icon_left: "key-variant"
                hint_text_color_normal: "blue"

            MDTextField:
                id: data
                size_hint_x: .8
                hint_text: "Data de nascimento:"
                helper_text: "dd/mm/yyyy"
                validator: "date"
                date_format: "dd/mm/yyyy"
                pos_hint: {'center_x': .5, 'center_y': .35}
                icon_left: "calendar-today"

            MDRaisedButton:
                size_hint_x: .8
                pos_hint: {'center_x': .5, 'center_y': .25}
                text: 'Criar Conta'
                md_bg_color: '#2d2734'


